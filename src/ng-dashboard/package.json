{
  "name": "ng-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-update": "ng update @angular/cdk @angular/cli @angular/core @angular/material --allow-dirty",
    "prestart": "node aspnetcore-https",
    "start": "ng serve --port 7121 --ssl --host 0.0.0.0 --ssl-cert cert/ng-dashboard.pem --ssl-key cert/ng-dashboard.key",
    "build": "ng build",
    "build-staging": "ng build --configuration staging",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "install-prereq": "npm install -g @angular/cli rimraf",
    "restore": "npm install",
    "npm-clear": "rimraf node_modules",
    "ng-clear": "rimraf .angular",
    "refresh": "npm run npm-clear && npm run ng-clear && npm run install-prereq && npm run restore"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/cdk": "^15.2.3",
    "@angular/common": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/material": "^15.2.3",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "@sweetalert2/ngx-sweetalert2": "^12.1.0",
    "@sweetalert2/themes": "^5.0.15",
    "ag-grid-angular": "^29.0.0",
    "ag-grid-community": "^29.0.0",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "~7.5.0",
    "sweetalert2": "^11.7.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.8.2"
  }
}
